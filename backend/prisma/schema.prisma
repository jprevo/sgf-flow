// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Game {
  id         String   @id
  playedAt   DateTime
  round      String
  event      String
  komi       String
  white      String
  black      String
  whiteRank  String
  blackRank  String
  whiteWins  Boolean
  blackWins  Boolean
  result     String
  createdAt  DateTime @default(now())
}
